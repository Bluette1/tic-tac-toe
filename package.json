{
  "name": "tic-tac-toe",
  "version": "1.0.0",
  "description": "This is an implementation of tic-tac-toe using JavaScript OOP design patterns",
  "main": "./src/index.js",
  "scripts": {
    "watch": "webpack --mode=development --watch",
    "build": "webpack -p --env production",
    "dev": "webpack-dev-server --mode=development --env development --watch --open",
    "debug": "webpack-dev-server  --open",
    "start": "webpack-dev-server --env production --open",
    "test": "jest",
    "eslint-f": "npx eslint . --fix",
    "stylelint-f": "npx stylelint **/*.{css,scss} --fix",
    "run-heroku": "yarn build && NODE_ENV=production node server.js"
  },
  "keywords": [
    "vanilla-javascript",
    "array",
    "javascript design patterns",
    "objects",
    "factories",
    "jest"
  ],
  "author": "descholar-ceo <emmanuellamugi@gmail.com> and marylene <marylene.sawyer@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "babel-eslint": "^10.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "html-webpack-plugin": "^4.5.0",
    "identity-obj-proxy": "^3.0.0",
    "mini-css-extract-plugin": "^1.0.0",
    "node-sass": "^4.14.1",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-csstree-validator": "^1.8.0",
    "stylelint-scss": "^3.18.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "babel-loader": "^8.1.0",
    "bootstrap": "^4.5.2",
    "css-loader": "^4.3.0",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "file-loader": "^6.1.1",
    "jest": "^26.6.1",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1",
    "sass-loader": "^10.0.3",
    "style-loader": "^2.0.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "transform": {
        "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/fileTransformer.js"
      }
    }
  }
}
